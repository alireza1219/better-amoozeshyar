{

  "description": "Enhancing your experience with amoozeshyar",
  "manifest_version": 2,
  "name": "advanced-amoozeshyar",
  "version": "0.2",
  "homepage_url": "https://github.com/alireza1219/advanced-amoozeshyar",

  "content_scripts": [
    {
      "matches": ["*://stdn.iau.ir/*"],
      "css": ["style.css"],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["*://stdn.iau.ir/Student/handleCourseClassSearchAction.do*",
        "*://stdn.iau.ir/Student/pSearchAction.do*"],
      "js": ["correction.js"]
    },
    {
      "matches": ["*://stdn.iau.ir/Student/manageAccount"],
      "js": ["cleartimer.js"]
    }
  ]
  
}